<AnchorPane id="AnchorPane" accessibleRole="BUTTON" prefHeight="293.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.una.examen.cliente.controller.CantonDetalleController">
   <children>
      <ImageView accessibleRole="BUTTON" fitHeight="45.0" fitWidth="49.0" layoutX="400.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@recursos/eliminar.png" />
         </image>
      </ImageView>
      <Label fx:id="lblInfo" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="296.0" text="Información">
         <font>
            <Font name="Candara" size="20.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtNombre" focusColor="#6dc36d" layoutX="62.0" layoutY="160.0" prefHeight="45.0" prefWidth="342.0" promptText="Nombre del Cantón">
         <font>
            <Font name="Candara" size="16.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="btnGuardar" layoutX="309.0" layoutY="228.0" onAction="#actGuardar" prefHeight="35.0" prefWidth="95.0" style="-fx-background-color: #6dc36d;" text="Guardar" />
