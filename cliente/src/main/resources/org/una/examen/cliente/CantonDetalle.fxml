      </JFXButton>
      <JFXComboBox fx:id="cbProvincia" focusColor="#6dc36d" layoutX="62.0" layoutY="90.0" onAction="#actProvincia" prefHeight="45.0" prefWidth="342.0" promptText="Provincia" />
   </children>
</AnchorPane>
